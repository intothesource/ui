    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/angular": "^5.3.18",
    "@types/classnames": "^2.2.10",
    "@types/debug": "^4.1.5",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.37",
    "@types/shortid": "0.0.29",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.2.2",
    "concurrently": "^5.1.0",
    "gh-pages": "^2.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
